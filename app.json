{
  "expo": {
    "name": "beauty_soft",
    "slug": "beauty_soft",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "BeautySoft",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "permissions": ["CAMERA"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.beauty_soft"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "cover",
          "enableFullScreenImage_legacy": true
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Inter-Bold.ttf",
            "./assets/fonts/Inter-Black.ttf",
            "./assets/fonts/Inter-ExtraBold.ttf",
            "./assets/fonts/Inter-Light.ttf",
            "./assets/fonts/Inter-ExtraLight.ttf",
            "./assets/fonts/Inter-Medium.ttf",
            "./assets/fonts/Inter-Regular.ttf",
            "./assets/fonts/Inter-SemiBold.ttf",
            "./assets/fonts/PlayfairDisplay-Black.ttf",
            "./assets/fonts/PlayfairDisplay-BlackItalic.ttf",
            "./assets/fonts/PlayfairDisplay-Bold.ttf",
            "./assets/fonts/PlayfairDisplay-BoldItalic.ttf",
            "./assets/fonts/PlayfairDisplay-ExtraBold.ttf",
            "./assets/fonts/PlayfairDisplay-Medium.ttf",
            "./assets/fonts/PlayfairDisplay-MediumItalic.ttf",
            "./assets/fonts/PlayfairDisplay-Regular.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "07d8e01e-7439-4def-8bf3-81fbc59a3818"
      }
    },
    "owner": "amhii"
  }
}
